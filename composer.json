{
    "name": "customer-vendor-member/member",
    "description": "Service for Customer",
    "type": "library",
    "authors": [
        {
            "name": "Plitakov Dmitry",
            "email": "plitakov94@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "nnx-skeleton-member/member": "dev-master"
    },
    "minimum-stability": "dev",

    "require-dev": {
        "fabpot/php-cs-fixer": "~1.11.0",
        "phpunit/phpunit": "~4.8.0",
        "phpunit/phpcov": "~2.0.0",
        "satooshi/php-coveralls": "~1.0.0",
        "zendframework/zend-loader": "~2.5.0"
    },
    "autoload": {
        "psr-4": {
            "CustomerVendorMember\\Member\\": "src/"
        },
        "classmap": [
            "./Module.php",
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CustomerVendorMember\\Member\\PhpUnit\\Test\\": "test/phpunit/tests",
            "CustomerVendorMember\\Member\\PhpUnit\\TestData\\": "test/phpunit/_files"
        },
        "classmap": [
            "./test/phpunit/Bootstrap.php",
            "test/"
        ]
    }
}
